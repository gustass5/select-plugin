{
  "name": "SelectHighlighter",
  "version": "1.0",
  "description": "Highlight matching",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/iconmonstr-selection-17-16.png",
      "32": "icons/iconmonstr-selection-17-32.png",
      "48": "icons/iconmonstr-selection-17-48.png",
      "128": "icons/iconmonstr-selection-17-120.png"
    }
  },
  "icons": {
    "16": "icons/iconmonstr-selection-17-16.png",
    "32": "icons/iconmonstr-selection-17-32.png",
    "48": "icons/iconmonstr-selection-17-48.png",
    "128": "icons/iconmonstr-selection-17-120.png"
  },
  "manifest_version": 2
}
